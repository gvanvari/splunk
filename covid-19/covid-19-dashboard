      <map>
        <search>
          <query>index=main sourcetype="csv" source="time_series_covid19_confirmed_us.csv" Lat!=0 Long!=0
| table Province_State , Lat , Long</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <sampleRatio>1</sampleRatio>
        </search>
